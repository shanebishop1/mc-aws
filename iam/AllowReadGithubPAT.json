{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "AllowReadGithubPAT",
			"Effect": "Allow",
			"Action": [
				"ssm:GetParameter"
			],
			"Resource": "arn:aws:ssm:us-west-1:123456789012:parameter/minecraft/github-pat"
		},
		{
			"Sid": "AllowDecryptSSMKey",
			"Effect": "Allow",
			"Action": [
				"kms:Decrypt"
			],
			"Resource": "*"
		}
	]
}
